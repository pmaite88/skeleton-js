    "date-fns": "^2.0.0-alpha.7",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "next": "^7.0.0",
    "next-with-apollo": "^3.1.3",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.1",
    "react-dom": "^16.5.2",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "graphql-tools": "^4.0.0"
